mu <- 26.3;
alpha <- 0.05;
#Ler dados de entrada
dados <- read.table("data.csv", header=FALSE, sep=";");
#CÃƒÂ¡lculo do BMI
BMI <- dados[1]/(dados[2]^2);
#NÃƒÂºmero de Amostras
n <- nrow(BMI);
#MÃƒÂ©dia
x_bar <- mean(as.matrix(BMI));
#Tamanho de Efeito
size_effect <- x_bar - mu;
#Desvio padrÃƒÂ£o
s <- sqrt(sum((BMI-x_bar)^2)/(n-1));
#t crÃƒ­tico
t_alpha <- qt(alpha/2, n-1);
#Intervalo de confianÃƒÂ§a
inter_min <- x_bar + (s*t_alpha / (sqrt(n)));
inter_max <- x_bar - (s*t_alpha / (sqrt(n)));
message('Valor de mu: ', mu);
message('Valor de alpha: ', alpha);
message('NÃƒÂºmero de amostras: ', n);
message('MÃƒÂ©dia amostral: ', x_bar);
message('Desvio padrÃƒÂ£o: ', s);
message('Tamanho de efeito: ', size_effect);
message('T crÃƒ­tico: ', t_alpha);
message('Intervalo de confianÃƒÂ§a: ', inter_min, ' a ', inter_max);
